<Page x:Class="Final.Pages.SecondPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Final.Pages"
      mc:Ignorable="d"
      d:DesignHeight="300" d:DesignWidth="300"
      Title="SecondPage"
      DataContext="{Binding Source={StaticResource Locator}, Path=Second}">

    <StackPanel>
        <Label Content="Second Page" FontSize="34" />
        <Label Height="45" />
        <Button Content="Go to MainPage" Command="{Binding ChangeToMainPageCommand}" />
    </StackPanel>
</Page>